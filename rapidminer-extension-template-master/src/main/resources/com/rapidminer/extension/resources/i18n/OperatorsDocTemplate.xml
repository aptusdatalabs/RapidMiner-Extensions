<?xml version="1.0" encoding="windows-1252" standalone="no"?>
<operatorHelp lang="en_EN">
    <!--  This is an example how to specify the translation of operator keys to names. -->
    <!--
    <operator>
      <key>example_operator_key</key>
      <name>Example Operator Name</name>
    </operator>
    -->

    <!-- This is how group keys are translated: -->
    <!--
    <group>
      <key>example_group</key>
      <name>Example Group</name>
    </group>
    -->
    
    <operator>
      <key>my_operator</key>
      <name>Find Posts</name>
    </operator>
    <operator>
      <key>my_operator2</key>
      <name>Find Pages</name>
    </operator>
    <operator>
      <key>fb_conn</key>
      <name>FB Connection</name>
    </operator>
    
    <group>
      <key>operator_test</key>
      <name>Example Group</name>
    </group>

</operatorHelp>
